{
	"arrowParens": "avoid",
	"quoteProps": "consistent",
	"trailingComma": "es5",
	"useTabs": true,
	"tabWidth": 2,
	"printWidth": 120,
	"alignInGroups": "always",
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"plugins": ["prettier-plugin-svelte", "@huggingface/prettier-plugin-vertical-align"]
}
